<csr:rule name="custom.rg.EnqueRule" match="custom:rg:enque" textonly>
	
	<csr:class super="dt.common.page.Rule" />
	
	<csr:description>
		
		<h1>custom.rg.EnqueRule</h1>
		
		<ul>
			<li>Version: v1.0.0</li>
			<li>Date: Monday, November 24, 2012</li>
			<li>Docs: <a href="https://github.com/registerguard/csp-enque">https://github.com/registerguard/csp-enque</a></li>
		</ul>
		
		<p>The <code>&lt;custom:rg:enque&gt;</code> adds HTML to the document <code>&gt;head&lt;</code>.</p>
		
	</csr:description>
	
	<csr:attribute name="position" description="Relative position in section. Default is 0. Negative is beginning of section or positive is end of section."                                   type="position:NUMBER" />
	<csr:attribute name="uglify"   description="Trim leading/trailing spaces; remove horizontal tabs, line feeds and carriage returns. This attribute doesn't (currently) take any arguments." type="uglify:BOOLEAN"  />
	
	<csr:action>
		
		<script language="cache" runat="compiler">
			do ##this.RenderDTStartTag()
		</script>
		
		<csr:section name="HEAD" block="##(##this.GetAttribute("position"))##">
			
			#[ new:( ' $data(enque)) enque set enque = ##class(custom.rg.Enque).html($get(enque), ##'..InnerText()'##, ##'..IsDefined("uglify")'##) ]#
			
		</csr:section>
		
		<csr:section name="POSTHTML">
			
			#[ kill:($data(enque)) enque ]#
			
		</csr:section>
		
		<script language="cache" runat="compiler">
			do ##this.RenderDTEndTag()
		</script>
		
	</csr:action>
	
</csr:rule>

